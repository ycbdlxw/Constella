{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      { "Name": "Console" }
    ],
    "Enrich": [ "FromLogContext" ]
  },
  "AllowedHosts": "*",
  "Database": {
    "Type": "Sqlite"
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=Data/myapp.db"
  }
}
